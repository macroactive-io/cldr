{
	"name": "macroactive/cldr",
	"description": "A lightweight localization database and translation tools, with data from the CLDR, IANA, ISO, etc.",
	"type": "library",
	"keywords": [
		"localization", "locale", "language", "script", "translate", "translation", "i18n", "l10n", "cldr"
	],
	"homepage": "https://github.com/macroactive-io/cldr",
	"license": "GPL-3.0-or-later",
	"authors": [{
			"name": "Greg Roach",
			"email": "greg@subaqua.co.uk",
			"role": "Developer"
	},{
			"homepage": "https://github.com/macroactive-io/cldr/graphs/contributors",
			"name": "Opensource contributors",
			"role": "Developer"
	}],
	"support": {
			"issues": "https://github.com/macroactive-io/cldr/issues"
	},
	"require": {
		"php": ">=8.1.0"
	},
	"require-dev": {
		"ext-simplexml": "*",
		"ext-zip": "*",
		"phpstan/phpstan": "*",
		"phpstan/extension-installer": "*",
		"phpstan/phpstan-deprecation-rules": "*",
		"phpstan/phpstan-strict-rules": "*",
		"phpstan/phpstan-phpunit": "*",
		"phpunit/phpunit": "*",
		"yoast/phpunit-polyfills":  "*",
		"php-coveralls/php-coveralls": "*",
		"friendsofphp/php-cs-fixer": "^3.12",
		"vimeo/psalm": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Macroactive\\Cldr\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Macroactive\\Cldr\\Tests\\": "test/"
		}
	},
	"config": {
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	}
}
